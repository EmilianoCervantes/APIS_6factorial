package org.drools.workshop.model;

import javax.xml.bind.annotation.XmlRootElement;

/**
 *
 * @author ralozano
 */
@XmlRootElement
public class Hermano {
    
    private String per1;
    private String per2;

    public Hermano() {
    }

    public Hermano(String per1, String per2) {
	this.per1 = per1;
	this.per2 = per2;
    }

    public String getPer1() {
        return per1;
    }

    public void setPer1(String per1) {
        this.per1 = per1;
    }

    public String getPer2() {
        return per2;
    }
    
    public void setPer2(String per2) {
        this.per2 = per2;
    }
   
    @Override
    public String toString() {
        return "Hermano{" + "per1=" + per1 + ", per2=" + per2 + '}';
    }
}
